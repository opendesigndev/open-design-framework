{
  "$schema": "https://typedoc.org/schema.json",
  "entryPointStrategy": "packages",
  "entryPoints": [
    "../opendesign-react",
    "../opendesign-universal",
    "./dom",
    "./external/*"
  ],
  "plugin": [
    "typedoc-plugin-resolve-crossmodule-references",
    "typedoc-plugin-mdn-links"
  ],
  "out": "dist",
  "json": "dist/docs.json",
  "readme": "README.md",
  "excludePrivate": true,
  "includes": ".."
}
